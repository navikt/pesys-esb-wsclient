<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://nav.no/virksomhet/tjenester/behandleTrekk/meldinger/v1" version="1.0" xmlns:meldinger="http://nav.no/virksomhet/tjenester/behandleTrekk/meldinger/v1" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:complexType name="Fagomrade">
